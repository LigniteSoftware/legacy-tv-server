{
    "asi": false,
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": false,
    "eqnull": true,
    "esnext": true,
    "freeze": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "laxbreak": true,
    "laxcomma": true,
    "newcap": true,
    "node": true,
    "maxparams": 3, // if you need more, use an options object
    "maxdepth": 4,
    "maxlen": 85,
    "noarg": true,
    "noyield": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "validthis": true,
    "white": true
}
